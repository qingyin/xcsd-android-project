                        android:textColor="#ff6f84"
                        android:textSize="24sp" />

                    <TextView
                        android:id="@+id/tv_space_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_space"
