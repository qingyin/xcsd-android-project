            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ll_logic"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp">

                <com.tuxing.app.Radar.RoundCornerProgressBar
                    android:id="@+id/progress_logic"
