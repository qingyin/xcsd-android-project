                    android:textSize="12sp" />

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/ll_space_score"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="22dp">

                <com.tuxing.app.Radar.RoundCornerProgressBar
                    android:id="@+id/progress_space_score"
                    app:rcMax="100"
                    app:rcProgress="0"
                    app:rcProgressColor="@color/custom_progress_orange_progress"
                    app:rcSecondaryProgress="86"
                    app:rcSecondaryProgressColor="@color/gray_line"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="26dp"
                    android:layout_marginRight="26dp" />

                <TextView
                    android:id="@+id/tv_space_left_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
