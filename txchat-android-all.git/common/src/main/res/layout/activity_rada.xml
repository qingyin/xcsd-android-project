                        android:maxEms="10"
                        android:text="空间思维"
                        android:textColor="#ff6f84"
                        android:textSize="12sp" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_26"
                    android:layout_marginTop="120dp">

                    <TextView
                        android:id="@+id/tv_logic"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:maxEms="10"
                        android:text="0"
                        android:textColor="#9a93f3"
                        android:textSize="24sp" />

                    <TextView
                        android:id="@+id/tv_logic_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_logic"
                        android:lines="1"
                        android:maxEms="10"
                        android:text="逻辑力"
                        android:textColor="#9a93f3"
                        android:textSize="12sp" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dp_26"
                    android:layout_marginTop="124dp">

                    <TextView
                        android:id="@+id/tv_response"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:maxEms="10"
                        android:text="0"
                        android:textColor="#ffc24b"
                        android:textSize="24sp" />

                    <TextView
                        android:id="@+id/tv_response_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_response"
                        android:lines="1"
                        android:maxEms="10"
                        android:text="反应力"
                        android:textColor="#ffc24b"
                        android:textSize="12sp" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="@dimen/dp_12"
                    android:layout_marginLeft="@dimen/dp_70">

                    <TextView
                        android:id="@+id/tv_attention_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:maxEms="10"
                        android:text="0"
                        android:textColor="#84bef0"
                        android:textSize="24sp" />

                    <TextView
                        android:id="@+id/tv_attention"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_attention_name"
                        android:lines="1"
                        android:maxEms="10"
                        android:text="注意力"
                        android:textColor="#84bef0"
                        android:textSize="12sp" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginBottom="@dimen/dp_12"
                    android:layout_marginRight="@dimen/dp_70">

                    <TextView
                        android:id="@+id/tv_memory_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:maxEms="10"
                        android:text="0"
                        android:textColor="#82d04d"
                        android:textSize="24sp" />

                    <TextView
                        android:id="@+id/tv_memory"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_memory_name"
                        android:lines="1"
                        android:maxEms="10"
                        android:text="记忆力"
                        android:textColor="#82d04d"
                        android:textSize="12sp" />

                </RelativeLayout>

                <RelativeLayout
