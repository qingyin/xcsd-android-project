<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin">

    <RelativeLayout
        android:id="@+id/rlTitle"
        android:layout_width="fill_parent"
        android:layout_height="48dp"
        android:background="@color/white">

        <RelativeLayout
            android:id="@+id/rl_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <RelativeLayout
                android:id="@+id/test_back_img_layout"
                android:layout_width="25dip"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/test_back_img"
                    android:layout_width="12dip"
                    android:layout_height="22dip"
                    android:layout_centerInParent="true"
                    android:background="@color/transparent"
                    android:src="@drawable/arrow_left_p" />
            </RelativeLayout>

            <TextView
                android:id="@+id/test_back_txt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_toRightOf="@id/test_back_img_layout"
                android:gravity="center_vertical"
                android:text="@string/back"
                android:textColor="@color/text_parent"
                android:textSize="16dip" />
        </RelativeLayout>

        <TextView
            android:id="@+id/tvTitle"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:ellipsize="end"
            android:gravity="center_vertical"
            android:maxEms="10"
            android:singleLine="true"
            android:text="小球的学能成绩"
            android:textColor="@color/black"
            android:textSize="18sp" />


        <TextView
            android:id="@+id/bt_right"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:paddingLeft="10dp"
            android:paddingRight="20dp"
            android:text="权威解释"
            android:textColor="@color/text_parent"
            android:textSize="@dimen/dp_14"
            android:visibility="visible" />

        <View
            android:layout_width="fill_parent"
            android:layout_height="@dimen/dp_0.5"
            android:layout_alignParentBottom="true"
            android:background="@color/bg" />
    </RelativeLayout>

    <com.tuxing.app.view.NewScrollView
        android:id="@+id/NewScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
