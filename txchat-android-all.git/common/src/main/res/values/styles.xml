    <style name="transparentDialogViewStyle" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/none</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowAnimationStyle">@style/dialog_animation</item>
    </style>

    <!-- 分享对话框imageview样式 -->
    <style name="dialog_content_share_image">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/square_white_share_icon_bg_selector</item>
